{
  "build": {
    "devPath": "../Frontend",
    "distDir": "../Frontend",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "BaueLogika",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "dialog": {
        "open": true
      },
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "copyright": "2024",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.tauri.dev",
      "targets": "all"
      },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "BaueLogika",
        "width": 800
      }
    ]
  }
}
